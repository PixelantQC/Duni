@objects
    #items
    image-item-*               css      div.qbank_griditem__item


= Item section =
    @for [1 - 5, 7 - 11] as index
        image-item-${index}:
            width 165 px
            height 231 px
            aligned horizontally all image-item-${index + 1}
            aligned vertically all image-item-${index + 6}
            near image-item-${index + 1} 28 to 36px left
